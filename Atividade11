// Calcule e mostre a tabuada de multiplicação de um número informado pelo usuário no teclado.

#include <stdio.h>

int main() {
    int numero, i;

    printf("Insira um número: ");
    scanf("%d", &numero);

    for (i = 0; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    return 0;
}
